<?xml version="1.0" encoding="UTF-8"?>
<!-- // suite name có thể chuyển thành project name , name = "Suite" -> name = "Nopcommerce" , parallel bỏ đi hoặc để đó cũng đc  -->

<suite parallel="false" name="NopCommerce">

<!-- // test name là mình chạy nó trên môi trường nào  --> 

  <test name="Run On Firefox">
    <classes>
      <class name="com.nopcommerce.user.User_01_Register_Login"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="Run On Chrome">
    <classes>
      <class name="com.nopcommerce.user.User_01_Register_Login"/>
    </classes>
  </test> 
  
  <test name="Run On IE">
    <classes>
      <class name="com.nopcommerce.user.User_02_MyAccount"/>
    </classes>
  </test> 
  
</suite> <!-- Suite -->
