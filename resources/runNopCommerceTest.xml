<?xml version="1.0" encoding="UTF-8"?>
<!-- // suite name có thể chuyển thành project name , name = "Suite" -> name 
	= "Nopcommerce" , parallel bỏ đi hoặc để đó cũng đc -->

<suite parallel="false" name="NopCommerce">


	<!-- // test name là mình chạy nó trên môi trường nào -->
	<test name="Run On Firefox">
	
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="javaBasic.Topic_06_Condition_Statement" />
		</classes>
		
	</test> <!-- Test -->

	<test name="Run On Chrome">

		<parameter name="browser" value="chrome" />
		<classes>
			<class name="javaBasic.Topic_06_Condition_Statement" />
		</classes>

	</test>

	<test name="Run On IE">

		<parameter name="browser" value="edge" />
		<classes>
			<class name="javaBasic.Topic_06_Condition_Statement" />
		</classes>

	</test>

</suite> <!-- Suite -->
