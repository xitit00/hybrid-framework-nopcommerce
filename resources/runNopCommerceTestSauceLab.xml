<?xml version="1.0" encoding="UTF-8"?>
<!-- // suite name có thể chuyển thành project name , name = "Suite" -> name 
	= "Nopcommerce" , parallel bỏ đi hoặc để đó cũng đc -->

<suite parallel="false" name="NopCommerce">

	<parameter name="url" value="https://demo.nopcommerce.com/" />
	
	<listeners>
		<listener class-name="common.MethodListener" />
		<listener class-name="reportConfig.ReportNGListener"/>
		<!--<listener class-name="reportConfig.AllureTestListener"/>-->
		<listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	</listeners>
	
	<test name="Run On Chrome">
	
		<parameter name="browserName" value="chrome" />
		<parameter name= "platForm" value="Windows 10" />
		<parameter name="browserVersion" value="latest" />
		
		<classes>
			<!-- BrowserStack Cloud Testing -->
			<class name="com.nopcommerce.user.Level_22_Sauce_Lab" />
			
		</classes>
		
	</test>
	
	<test name="Run On Firefox">
	
		<parameter name="browserName" value="firefox" />
		<parameter name= "platForm" value="macOS 11.00" />
		<parameter name="browserVersion" value= "latest" />
		
		<classes>
			<!-- BrowserStack Cloud Testing -->
			<class name="com.nopcommerce.user.Level_22_Sauce_Lab" />
			
		</classes>
		
	</test>

